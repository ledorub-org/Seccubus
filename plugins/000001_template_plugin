# Template plugin for Seccubus 
# ------------------------------------------------------------------------------
# name:    Template
# scanner: OpenVAS
# state:   enabled 
# ------------------------------------------------------------------------------

sub {
    my $ref = ${ shift };

    $$ref -> {finding_txt} =~ /(CVE-\d+-\d+)/;
    $$ref -> {port} = '8090';
    print $1 . "\n"; 

    print Dumper $ref;

    return 0;
}